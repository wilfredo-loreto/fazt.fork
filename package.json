{
  "name": "fazt-api",
  "version": "1.1.0",
  "description": "Api restfull for community of Fazt",
  "keywords": [
    "api",
    "restfull",
    "nodejs",
    "typescript",
    "community",
    "fazt"
  ],
  "license": "MIT",
  "private": false,
  "main": "./dist/index.js",
  "scripts": {
    "lint:prettier": "prettier --write \"**/*.{js,ts,json,md}\"",
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpileOnly src/index.ts",
    "start": "node dist/index.js",
    "clean": "rimraf dist docs",
    "release": "standard-version",
    "docs": "apidoc -i src -o docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/faztdevelopers/fazt-api.git"
  },
  "bugs": {
    "url": "https://github.com/faztdevelopers/fazt-api/issues"
  },
  "homepage": "https://github.com/faztdevelopers/fazt-api#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm run build",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.6.0",
    "helmet": "^3.23.3",
    "http-status-codes": "^1.4.0",
    "husky": "^4.2.5",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.22",
    "morgan": "^1.10.0",
    "standard-version": "^8.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@commitlint/config-lerna-scopes": "^8.1.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.7",
    "@types/helmet": "0.0.47",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.7.30",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.0.19",
    "@types/swagger-ui-express": "^4.1.2",
    "apidoc": "^0.23.0",
    "nodemon": "^2.0.4",
    "prettier": "2.0.5",
    "rimraf": "^3.0.2",
    "swagger-ui-express": "^4.1.4",
    "ts-node-dev": "^1.0.0-pre.50",
    "typescript": "^3.9.6"
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0"
  }
}
